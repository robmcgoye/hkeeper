<%= form_with model: computer, :data => {:turbo => "false"} do |f| %>
  <%= render 'shared/error_messages', resource: computer %>

  <div class="field form-group">
    <%= f.label "Notes" %><br />
    <%= f.text_area :notes, class: "form-control", rows: 8 %>
  </div>  
  <div class="actions pt-2">
    <%= f.submit class: "btn btn-sm btn-outline-dark" %>
  </div>

<% end %>
